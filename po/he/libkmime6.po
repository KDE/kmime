#: kmime_dateformatter.cpp:161
#, kde-format
msgctxt "invalid time specified"
msgid "unknown"
msgstr "לא ידוע"

#: kmime_dateformatter.cpp:175
#, kde-format
msgid "Today %1"
msgstr "היום %1"

#: kmime_dateformatter.cpp:179
#, kde-format
msgid "Yesterday %1"
msgstr "אתמול %1"

#: kmime_dateformatter.cpp:184
#, kde-format
msgctxt "1. weekday, 2. time"

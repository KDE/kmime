#, kde-format
msgctxt "invalid time specified"
msgid "unknown"
msgstr "nepoznat"

# >> @item Fancy date
#: kmime_dateformatter.cpp:210
#, kde-format
msgid "Today %1"
msgstr "danas %1"

# >> @item Fancy date
#: kmime_dateformatter.cpp:214
#, kde-format
msgid "Yesterday %1"
msgstr "juče %1"

# >> @item Fancy date: %1 weekday, %2 time
#: kmime_dateformatter.cpp:219
#, kde-format
msgctxt "1. weekday, 2. time"

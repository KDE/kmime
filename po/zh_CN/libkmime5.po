#: kmime_dateformatter.cpp:247
#, kde-format
msgctxt "invalid time specified"
msgid "unknown"
msgstr "未知"

#: kmime_dateformatter.cpp:263
#, kde-format
msgid "Today %1"
msgstr "今天 %1"

#: kmime_dateformatter.cpp:267
#, kde-format
msgid "Yesterday %1"
msgstr "昨天 %1"

#: kmime_dateformatter.cpp:272
#, kde-format
msgctxt "1. weekday, 2. time"
